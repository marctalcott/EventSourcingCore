{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Azure": {
    "EventContainer": "events",
    "ViewContainer": "views",
    "SnapshotContainer": "snapshots",
    "Database": "myCosmosDb",
    "AuthKey": "[your auth key]",
    "EndPointUri": "[your cosmosdb url]"
  },
  "Auth0": {
    "RegistrationUrl": "[your auth0 domain]/dbconnections/signup",
    "LoginUri":  "[your login url]/oauth/token",
    "Audience": "[your audience url]",
    "ClientId": "[clientId]",
    "ClientSecret": "[clientSecret]",
    "Connection": "Username-Password-Authentication",
    "Authority": "https://[yourAuth].auth0.com/"
  }
}
